# Database Configuration
DATABASE_URL=your_neon_database_url_here
PGDATABASE=your_database_name
PGHOST=your_database_host
PGPASSWORD=your_database_password
PGPORT=5432
PGUSER=your_database_user

# Session Configuration
SESSION_SECRET=your_session_secret_here

# Replit Configuration (automatically set in Replit)
REPLIT_DOMAINS=your_replit_domain.replit.app
REPL_ID=your_repl_id
ISSUER_URL=https://replit.com/oidc

# Auth0 Configuration (Simplified Google OAuth Only)
# Set these in Replit Secrets or your .env file
VITE_AUTH0_DOMAIN=your_auth0_domain.auth0.com
VITE_AUTH0_CLIENT_ID=your_auth0_client_id
AUTH0_CLIENT_SECRET=your_auth0_client_secret

# Note: No AUTH0_AUDIENCE needed for simplified Google OAuth integration